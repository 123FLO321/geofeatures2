{
  "name": "GeoFeatures2",
  "version": "2.0.0",
  "summary": "A lightweight, high performance geometry library for Swift",
  "homepage": "https://github.com/tonystone/geofeatures",
  "license": "Apache License, Version 2.0",
  "authors": {
    "Tony Stone": "https://github.com/tonystone"
  },
  "source": {
    "git": "https://github.com/tonystone/geofeatures2.git",
    "tag": "2.0.0"
  },
  "platforms": {
    "ios": "5.0",
    "osx": "10.7",
    "watchos": "2.0",
    "tvos": "9.0"
  },
  "requires_arc": true,
  "default_subspecs": "Geometry",
  "subspecs": [
    {
      "name": "Geometry",
      "source_files": "Source/Geometry/**/*.swift",
      "preserve_paths": "Source/Geometry/**/*.swift.gyb"
    },
    {
      "name": "Visualization",
      "source_files": "Source/Visualization/**/*.swift",
      "preserve_paths": "Source/Visualization/**/*.swift.gyb"
    }
  ]
}
